{
  "name": "Uniteams",
  "version": "1.0.0",
  "description": "Uniteams Nuxt.js project",
  "author": "Uniteams",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
